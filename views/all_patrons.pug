include partials/_head
include partials/_nav

h1 Patrons
p
  a.button(href='new_patron.html') Create New Patron
table
  thead
    tr
      th Name
      th Address
      th Email
      th Library ID
      th Zip
  tbody
    each patron in patrons
      tr
        td
          a(href='FIX THIS') #{patron.first_name} #{patron.last_name}
        td #{patron.address}
        td #{patron.email}
        td #{patron.library_id}
        td #{patron.zip_code}
